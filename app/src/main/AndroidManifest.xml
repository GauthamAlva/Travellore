<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.converty.travellore">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Temple_Class"
            android:label="Temple"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".Park_Class"
            android:label="Parks"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".History_Class"
            android:label="Historical"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".Beach_Class"
            android:label="Beaches"
            android:parentActivityName=".MainActivity">
        </activity>
    </application>

</manifest>